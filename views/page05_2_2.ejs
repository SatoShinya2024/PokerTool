      <div id="board" class="text-center container-fluid">
        <h2 class="display-1 text-dark">ボード</h2>
        <div class="justify-content-center row mb-2">
          <img src="/images/card-img/trumpdummy.JPG" class="col-2"  id="board_card1" name = "フロップ" onclick="card_click('board_card1', 0, 'board')">
