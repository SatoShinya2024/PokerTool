<%- include('header_05', title) %>
<main class="container-fluid mb-5">
  <h1 class="text-light display-1 text-center">ハンドヒストリーの登録</h1>
	<div class="container-fluid border border-5 rounded fs-1 text-start text-dark bg-light" id="setting_wrapper" style="margin: 0 auto;">
		<h2 class="display-3 text-center mb-3">ゲームの設定</h2>
    <div class="row mt-5 justify-content-center">
			<label class="form-label col-4">ゲームの種類:</label>
			<div class="col-7">
				<select name="game_type" id="game_type" class="form-select fs-1">
					<option value="キャッシュ">キャッシュ</option>
					<option value="トーナメント">トーナメント</option>
				</select>
			</div>
		</div>
    
    <div class="row mt-5 justify-content-center">
			<label class="form-label col-4">スタック量:</label>
			<div class="col-7">
				<input type="number" class="form-control fs-1" name="effective_stack" id="effective_stack"  placeholder="effective stackを入力" autocomplete="off" value=100 onchange="effectivestack_change()">
			</div>	
		</div>
    
    <div class="row mt-5 justify-content-center">
			<label class="form-label col-4">単位:</label>
			<div class="col-7">
				<select id="unit" class="form-select fs-1" onchange="unit_change()" name="unit">
					<option value="bb">bb</option>
					<option value="pt">pt</option>
					<option value="$">$</option>
				</select>
			</div>
		</div>
    
    
    <div class="row mt-5 justify-content-center">
			<label class="form-label col-4">ブラインド:</label>
			<div class="col-7 row justify-content-between">
				<div class="col-5">
					<input type="number" name="small_blind" id="small_blind" autocomplete="off" value=0.5 class="form-control fs-1">
				</div>
				<div class="col-2 text-center">/</div>
				<div class="col-5">
					<input type="number" name="big_blind" id="big_blind" autocomplete="off" value=1 class="form-control fs-1">
				</div>
			</div>
		</div>
    
    
    <div class="row mt-5 justify-content-center">
			<label class="form-label col-4">アンティ:</label>
			<div class="row col-7 justify-content-between">
				<div class="col">
					<select name="ante_who" id = "ante_who" class="form-select fs-1">
						<option value="none" selected disabled>誰が払う？</option>
						<option value="none">なし</option>
						<option value="BigBlind">BB</option>
						<option value="AllPlayer">全てのプレイヤー</option>
					</select>
				</div>
				<div class="col">
					<input type="number" class="form-control fs-1" name="ante_amount" id="ante_amount" list="ante" placeholder="いくら？" autocomplete="off">
					<datalist id="ante">
					<option value=1>
					<option value=0.5>
					<option value=0.2>
					<option value=0.1>
					</datalist>
				</div>
			</div>
		</div>
    </div>
    
    <div class="container-fluid text-dark bg-light border border-5 mt-5 text-center" id="players_wrapper">
      <h2 class="display-1 text-center text-dark mt-3 mb-3">ハンド登録</h2>
      <div id="players_hand" class="row justify-content-around align-items-center">
        <div class="col-4 border border-2" name="player-box">
          <div class="lead fs-1 text-dark">SB</div>
          <div class="mt-2">
            <input class = "form-control fs-1" name="personal_name" id = "SB_person" placeholder="プレイヤー名">
          </div>
          <div class="row justify-content-center mt-2">
            <img src="/images/card-img/trumpdummy.JPG" class="col-6" id="SB_card1" name="SB" onclick="card_click('SB_card1', 0, 'hand')"><img src="/images/card-img/trumpdummy.JPG" class="col-6" id="SB_card2" name="SB" onclick="card_click('SB_card2', 1, 'hand')">
          </div>
          <div class="mt-2"><button class="btn btn-secondary w-100 fs-2" onclick="card_clear(['SB_card1', 'SB_card2'], [0, 1], 'hand')">クリア</button></div>
          <div class="row align-items-center  justify-content-between mt-2 mb-2">
            <div class="col-8"><input type="number" value=100 class="form-control fs-1" name="player_stack" id="SB_stack"></div><div class="col-4 fs-1 text-start" name="player_unit">bb</div>
          </div>
        </div>
        <div class="col-4 border border-2" name="player-box">
          <div class="lead fs-1 text-dark">BB</div>
          <div class="mt-2">
            <input class = "form-control fs-1" name="personal_name" id = "BB_person" placeholder="プレイヤー名">
          </div>
          <div class="row justify-content-center mt-2">
            <img src="/images/card-img/trumpdummy.JPG" class="col-6" id="BB_card1" name="BB" onclick="card_click('BB_card1', 2, 'hand')"><img src="/images/card-img/trumpdummy.JPG" class="col-6" id="BB_card2" name="BB" onclick="card_click('BB_card2', 3, 'hand')">
          </div>
          <div class="mt-2"><button class="btn btn-secondary w-100 fs-2" onclick="card_clear(['BB_card1', 'BB_card2'], [2, 3], 'hand')">クリア</button></div>
          <div class="row align-items-center  justify-content-between mt-2 mb-2">
            <div class="col-8"><input type="number" value=100 class="form-control fs-1" name="player_stack" id="BB_stack"></div><div class="col-4 fs-1 text-start" name="player_unit">bb</div>
          </div>
        </div>
      </div>
    </div>
    
    <div id="canvas_wrapper" style="display: none;" class="container-fluid bg-light border-none mt-2">
      <canvas class="container-fluid" id="canvas" width="800" height="400">
        
      </canvas>
      
      <p class="text-center"><button class="btn btn-outline-info fs-1" onclick="video_draw('canvas', false)" >動画でアクションを確認する</button></p>
      <p class="text-center"><button class="btn btn-outline-secondary fs-1" onclick = "canvas_close()"  >画面を閉じる</button></p>
    </div>
  
    <div class="container-fluid mt-3 bg-light" id="large_wrapper">
      
